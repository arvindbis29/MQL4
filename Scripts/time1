#property strict
#property script_show_inputs
input  datetime x=D'2022.01.02 12:35:00';
input  datetime x_end=D'2022.01.02 12:35:00'; 

//+------------------------------------------------------------------+
//| Script program start function                                    |
//+------------------------------------------------------------------+
void OnStart()
  {
//---

   if(x<TimeCurrent()&&x_end>TimeCurrent())
   {
   Print("yes");
   }
   else
   Print("NO");
  }
