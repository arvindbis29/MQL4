//+------------------------------------------------------------------+
//| Custom indicator initialization function                         |
//+------------------------------------------------------------------+
int OnInit()
  {
//--- indicator buffers mapping
   ObjectDelete("q2");

   if(ObjectCreate(0,"q2",OBJ_RECTANGLE,0,Time[0],0,Time[10],18))
   {
   //--- set rectangle color
   ObjectSetInteger(0,"q2",OBJPROP_COLOR,clrRed);
   //--- set the style of rectangle lines
   ObjectSetInteger(0,"q2",OBJPROP_STYLE,STYLE_SOLID);
   //--- set width of the rectangle lines
   ObjectSetInteger(0,"q2",OBJPROP_WIDTH,2);
   }


//---
   return(INIT_SUCCEEDED);
  }
//+------------------------------------------------------------------+
//| Custom indicator iteration function                              |
//+------------------------------------------------------------------+
int OnCalculate(const int rates_total,
                const int prev_calculated,
                const datetime &time[],
                const double &open[],
                const double &high[],
                const double &low[],
                const double &close[],
                const long &tick_volume[],
                const long &volume[],
                const int &spread[])
  {
//---

//--- return value of prev_calculated for next call
   return(rates_total);
  }
